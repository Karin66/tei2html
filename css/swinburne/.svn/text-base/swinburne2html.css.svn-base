/* headings */
h1,h2,h3,h4,h5,h6,.genericHeading, .head {font-weight:bold; width:28em;}
h1 {
    font-size:140%;
    color:#351408;
    margin-bottom:0;
    page-break-after:avoid;
}
h1.small {
    font-size:small;
    color:#351408;
    margin-top:1em;
    margin-bottom:0;
    page-break-after:avoid;
}
h2 {
    font-size:100%;
    color:#351408;
    page-break-after:avoid;
}
/* Generic TEI head.  Modeled on h4, but black. */
.head {
    font-size:90%;
    page-break-after:avoid;
}
h3 {
    font-size:90%;
    color:#351408;
    font-style:italic;
    page-break-after:avoid;
}
h4 {
    font-size:85%;
    color:#351408;
    page-break-after:avoid;
}
.genericHeading {
    display:block;
    margin-bottom:1em;
    color:#351408;
/*    font-weight:bold; */
    page-break-after:avoid;
}
h5 {
    font-size:85%;
    color:#351408;
    page-break-after:avoid;
}
h6 {
    font-size:85%;
    color:#351408;
    page-break-after:avoid;
}
hr.h1 {
    text-align:left;
    color:#351408;
    width:90%;
    margin-top:0;
}
hr.figureBorder {
    text-align:center;
    width:80%;
    color:#351408;
    background-color:#351408;
}
.bibl {
